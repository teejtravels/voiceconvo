{
  "name": "voice-chatbot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ricky0123/vad": "^0.2.4",
    "axios": "^0.27.2",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "ngrok": "^5.0.0-beta.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.3"
  },
  "scripts": {
    "start": "concurrently \"yarn start:frontend\" \"yarn start:backend\"",
    "start:frontend": "react-scripts start",
    "start:backend": "cd server && node server.js",
    "install:all": "yarn install && cd server && yarn install",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  }
}
